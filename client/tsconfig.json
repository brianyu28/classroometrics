{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",

    "paths": {
      "crmet/*": ["./src/*"]
    },

    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "noImplicitAny": true
  }
}
